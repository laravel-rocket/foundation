{
    "name": "laravel-rocket/foundation",
    "description": "Core Components of Laravel Rocket",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Takaaki Mizuno",
            "email": "takaaki.mizuno@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/tinker": "^2.9",
        "icanboogie/inflector": "^2.2",
        "aws/aws-sdk-php": "^3.300",
        "rainner/restful-php": "^1.0",
        "maatwebsite/excel": "^3.1",
        "fzaninotto/faker": "^1.5",
        "symfony/thanks": "^1.2",
        "nunomaduro/collision": "^7.10",
        "filp/whoops": "^2.15"
    },
    "require-dev": {
        "laravel/laravel": "^10.0",
        "laravel/browser-kit-testing": "^7.2",
        "friendsofphp/php-cs-fixer": "^3.51",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelRocket\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true
        }
    }
}
