{
    "name": "laravel-rocket/foundation",
    "description": "Core Components of Laravel Rocket",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Takaaki Mizuno",
            "email": "takaaki.mizuno@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/tinker": "^2.9",
        "icanboogie/inflector": "^2.2",
        "aws/aws-sdk-php": "^3.321",
        "rainner/restful-php": "^1.0",
        "jolicode/slack-php-api": "^4.7",
        "ext-imagick": "*"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "symfony/thanks": "^1.0",
        "mockery/mockery": "~1.0",
        "friendsofphp/php-cs-fixer": "^3.25",
        "fakerphp/faker": "^1.23",
        "maatwebsite/excel": "~3.1.0",
        "laravel/framework": "^11.9",
        "laravel/browser-kit-testing": "^7.2",
        "nunomaduro/collision": "^8.0",
        "laravel/pint": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "LaravelRocket\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true,
            "php-http/discovery": true
        }
    }
}
